<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.ssafy.gumipresso.fragment.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_notiFragment"
            app:destination="@id/notiFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right" />
        <action
            android:id="@+id/action_homeFragment_to_cartFragment"
            app:destination="@id/cartFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
        <action
            android:id="@+id/action_homeFragment_to_recentOrderDetailFragment"
            app:destination="@id/recentOrderDetailFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
    </fragment>
    <fragment
        android:id="@+id/orderFragment"
        android:name="com.ssafy.gumipresso.fragment.OrderFragment"
        android:label="OrderFragment"
        tools:layout="@layout/fragment_order">
        <action
            android:id="@+id/action_orderFragment_to_cartFragment"
            app:destination="@id/cartFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right" />
        <action
            android:id="@+id/action_orderFragment_to_orderDetailFragment"
            app:destination="@id/orderDetailFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right" />
        <action
            android:id="@+id/action_orderFragment_to_mapsFragment"
            app:destination="@id/mapsFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right" />
    </fragment>
    <fragment
        android:id="@+id/recentOrderDetailFragment"
        android:name="com.ssafy.gumipresso.fragment.RecentOrderDetailFragment"
        android:label="RecentOrderDetailFragment"
        tools:layout="@layout/fragment_recent_order_detail">
        <action
            android:id="@+id/action_recentOrderDetailFragment_to_cartFragment"
            app:destination="@id/cartFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right" />
    </fragment>
    <fragment
        android:id="@+id/orderDetailFragment"
        android:name="com.ssafy.gumipresso.fragment.OrderDetailFragment"
        android:label="OrderDetailFragment"
        tools:layout="@layout/fragment_order_detail">
        <action
            android:id="@+id/action_orderDetailFragment_to_reviewWriteFragment"
            app:destination="@id/reviewWriteFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right" />
        <action
            android:id="@+id/action_orderDetailFragment_to_reviewModifyFragment"
            app:destination="@id/reviewModifyFragment" />
    </fragment>
    <fragment
        android:id="@+id/cartFragment"
        android:name="com.ssafy.gumipresso.fragment.CartFragment"
        android:label="CartFragment"
        tools:layout="@layout/fragment_cart" />
    <fragment
        android:id="@+id/myPageFragment"
        android:name="com.ssafy.gumipresso.fragment.MyPageFragment"
        android:label="MyPageFragment"
        tools:layout="@layout/fragment_my_page">
        <action
            android:id="@+id/action_myPageFragment_to_recentOrderDetailFragment"
            app:destination="@id/recentOrderDetailFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right" />
        <action
            android:id="@+id/action_myPageFragment_to_cartFragment"
            app:destination="@id/cartFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right" />
        <action
            android:id="@+id/action_myPageFragment_to_gradeFragment"
            app:destination="@id/gradeFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right" />
        <action
            android:id="@+id/action_myPageFragment_to_settingsFragment"
            app:destination="@id/settingsFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right" />
    </fragment>
    <fragment
        android:id="@+id/mapsFragment"
        android:name="com.ssafy.gumipresso.fragment.MapsFragment"
        android:label="MapsFragment"
        tools:layout="@layout/fragment_maps" />
    <fragment
        android:id="@+id/notiFragment"
        android:name="com.ssafy.gumipresso.fragment.NotiFragment"
        android:label="NotiFragment"
        tools:layout="@layout/fragment_noti" />
    <fragment
        android:id="@+id/reviewWriteFragment"
        android:name="com.ssafy.gumipresso.fragment.ReviewWriteFragment"
        android:label="ReviewWriteFragment"
        tools:layout="@layout/fragment_review_write" />
    <fragment
        android:id="@+id/gradeFragment"
        android:name="com.ssafy.gumipresso.fragment.GradeFragment"
        android:label="GradeFragment"
        tools:layout="@layout/fragment_grade" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.ssafy.gumipresso.fragment.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/payFragment"
        android:name="com.ssafy.gumipresso.fragment.PayFragment"
        android:label="fragment_pay"
        tools:layout="@layout/fragment_pay" />
    <fragment
        android:id="@+id/reviewModifyFragment"
        android:name="com.ssafy.gumipresso.fragment.ReviewModifyFragment"
        android:label="ReviewModifyFragment"
        tools:layout="@layout/fragment_review_modify"/>
</navigation>